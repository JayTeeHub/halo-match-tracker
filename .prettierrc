{
    "printWidth": 100,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "typescript",
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false,
    "overrides": [
        {
            "files":"*.ts",
            "options": {
                "parser": "typescript"
            }
        },
        {
            "files":"*.html",
            "options": {
                "parser": "html"
            }
        }
    ]
}